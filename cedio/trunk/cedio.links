#
#	Create a subdirectory for compilation and
#       set links by sourcing this set.links file.
#
set OS = `uname`
if ($OS == "OSF1")set mach = alpha
if ($OS == "SunOS")set mach = solaris
if ($OS == "Linux")set mach = linux
if ($OS == "IRIX")set mach = sgi
#
ln -s $HOME/Cedio/CEDLOG_others.f  CEDLOG.f
ln -s $HOME/Cedio/CEDRIC_WKSTN.INC CEDRIC.INC
ln -s $HOME/Cedio/cedric.h-$mach   cedric.h
ln -s $HOME/Cedio/*.[fch]          .
ln -s $HOME/Cedio/makefile_WKSTN   makefile
#